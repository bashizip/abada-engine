{
  "realm": "abada-dev",
  "enabled": true,
  "sslRequired": "none",
  "roles": {
    "realm": [
      {
        "name": "managers"
      },
      {
        "name": "customers"
      }
    ]
  },
  "groups": [
    {
      "name": "managers"
    },
    {
      "name": "customers"
    }
  ],
  "clients": [
    {
      "clientId": "abada-frontend",
      "enabled": true,
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "redirectUris": [
        "http://localhost:5602/*",
        "http://localhost:5603/*",
        "https://localhost/*",
        "https://tenda.localhost/*",
        "https://orun.localhost/*",
        "https://keycloak.localhost/*"
      ],
      "webOrigins": [
        "http://localhost",
        "https://localhost",
        "https://tenda.localhost",
        "https://orun.localhost",
        "https://keycloak.localhost"
      ],
      "protocol": "openid-connect",
      "protocolMappers": [
        {
          "name": "audience",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "abada-frontend",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "alice",
      "email": "alice@abada.io",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "alice",
          "temporary": false
        }
      ],
      "groups": [
        "managers"
      ]
    },
    {
      "username": "bob",
      "email": "bob@abada.io",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "bob",
          "temporary": false
        }
      ],
      "groups": [
        "customers"
      ]
    }
  ]
}