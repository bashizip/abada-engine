<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler">
  <bpmn:process id="OrderFulfillmentProcess" name="Order Fulfillment Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Order Received">
      <bpmn:outgoing>Flow_Start_SetDetails</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_SetDetails" name="Set Order Details" camunda:candidateUsers="test-user">
      <bpmn:incoming>Flow_Start_SetDetails</bpmn:incoming>
      <bpmn:outgoing>Flow_SetDetails_Gateway</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:inclusiveGateway id="Gateway_1" name="Check Order Details">
      <bpmn:incoming>Flow_SetDetails_Gateway</bpmn:incoming>
      <bpmn:outgoing>Flow_Physical</bpmn:outgoing>
      <bpmn:outgoing>Flow_Digital</bpmn:outgoing>
      <bpmn:outgoing>Flow_Discount</bpmn:outgoing>
    </bpmn:inclusiveGateway>
    <bpmn:userTask id="Activity_Physical" name="Process Physical Goods" camunda:candidateUsers="test-user">
      <bpmn:incoming>Flow_Physical</bpmn:incoming>
      <bpmn:outgoing>Flow_Physical_Join</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Activity_Digital" name="Process Digital Goods" camunda:candidateUsers="test-user">
      <bpmn:incoming>Flow_Digital</bpmn:incoming>
      <bpmn:outgoing>Flow_Digital_Join</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Activity_Discount" name="Apply Discount" camunda:candidateUsers="test-user">
      <bpmn:incoming>Flow_Discount</bpmn:incoming>
      <bpmn:outgoing>Flow_Discount_Join</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:inclusiveGateway id="Gateway_2" name="Join Order Processing">
      <bpmn:incoming>Flow_Physical_Join</bpmn:incoming>
      <bpmn:incoming>Flow_Digital_Join</bpmn:incoming>
      <bpmn:incoming>Flow_Discount_Join</bpmn:incoming>
      <bpmn:outgoing>Flow_Gateway_End</bpmn:outgoing>
    </bpmn:inclusiveGateway>
    <bpmn:endEvent id="EndEvent_1" name="Order Completed">
      <bpmn:incoming>Flow_Gateway_End</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_Start_SetDetails" sourceRef="StartEvent_1" targetRef="Task_SetDetails" />
    <bpmn:sequenceFlow id="Flow_SetDetails_Gateway" sourceRef="Task_SetDetails" targetRef="Gateway_1" />
    <bpmn:sequenceFlow id="Flow_Physical" name="Physical Goods" sourceRef="Gateway_1" targetRef="Activity_Physical">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${order.hasPhysicalItems}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Digital" name="Digital Goods" sourceRef="Gateway_1" targetRef="Activity_Digital">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${order.hasDigitalItems}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Discount" name="Has Discount" sourceRef="Gateway_1" targetRef="Activity_Discount">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${order.hasDiscountCode}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Physical_Join" sourceRef="Activity_Physical" targetRef="Gateway_2" />
    <bpmn:sequenceFlow id="Flow_Digital_Join" sourceRef="Activity_Digital" targetRef="Gateway_2" />
    <bpmn:sequenceFlow id="Flow_Discount_Join" sourceRef="Activity_Discount" targetRef="Gateway_2" />
    <bpmn:sequenceFlow id="Flow_Gateway_End" sourceRef="Gateway_2" targetRef="EndEvent_1" />
  </bpmn:process>
