<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Definitions_TimerTest" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Gemini">
  <bpmn:process id="TimerEventProcess" name="Timer Event Test Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start">
      <bpmn:outgoing>Flow_to_InitialTask</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="InitialTask" name="Initial Task" camunda:candidateUsers="test-user">
      <bpmn:incoming>Flow_to_InitialTask</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Timer</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:intermediateCatchEvent id="CatchEvent_Timer" name="Wait 1 Second">
      <bpmn:incoming>Flow_to_Timer</bpmn:incoming>
      <bpmn:outgoing>Flow_to_FinalTask</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerEventDefinition_1">
        <bpmn:timeDuration>PT1S</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:userTask id="FinalTask" name="Final Task" camunda:candidateUsers="test-user">
      <bpmn:incoming>Flow_to_FinalTask</bpmn:incoming>
      <bpmn:outgoing>Flow_to_End</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_to_End</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_to_InitialTask" sourceRef="StartEvent_1" targetRef="InitialTask" />
    <bpmn:sequenceFlow id="Flow_to_Timer" sourceRef="InitialTask" targetRef="CatchEvent_Timer" />
    <bpmn:sequenceFlow id="Flow_to_FinalTask" sourceRef="CatchEvent_Timer" targetRef="FinalTask" />
    <bpmn:sequenceFlow id="Flow_to_End" sourceRef="FinalTask" targetRef="EndEvent_1" />
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="TimerEventProcess">
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="152" y="252" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="InitialTask_di" bpmnElement="InitialTask">
        <dc:Bounds x="240" y="230" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="CatchEvent_Timer_di" bpmnElement="CatchEvent_Timer">
        <dc:Bounds x="402" y="252" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="FinalTask_di" bpmnElement="FinalTask">
        <dc:Bounds x="500" y="230" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <dc:Bounds x="662" y="252" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_to_InitialTask_di" bpmnElement="Flow_to_InitialTask">
        <di:waypoint x="188" y="270" /><di:waypoint x="240" y="270" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_to_Timer_di" bpmnElement="Flow_to_Timer">
        <di:waypoint x="340" y="270" /><di:waypoint x="402" y="270" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_to_FinalTask_di" bpmnElement="Flow_to_FinalTask">
        <di:waypoint x="438" y="270" /><di:waypoint x="500" y="270" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_to_End_di" bpmnElement="Flow_to_End">
        <di:waypoint x="600" y="270" /><di:waypoint x="662" y="270" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>